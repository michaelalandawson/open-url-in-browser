{
	"name": "open-url-in-browser",
	"publisher": "michaelalandawson",
	"displayName": "open-url-in-browser",
	"description": "Opens the current file as a URL in the default browser.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "open-url-in-browser.open",
				"title": "Open URL in Default Browser"
			}
		],
		"configuration": {
			"title": "Open URL in Default Browser",
			"properties": {
				"mappings.localRoot": {
					"description": "Local path to web site root.",
					"scope": "resource",
					"type": "string",
					"default": "[LOCAL_PATH]\\wwwroot"
				},
				"mappings.serverRoot": {
					"description": "URI to web site root.",
					"scope": "resource",
					"type": "string",
					"default": "https://[SERVER_ROOT]"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.74.0",
		"@types/glob": "^8.0.0",
		"@types/mocha": "^10.0.1",
		"@types/node": "16.x",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"eslint": "^8.28.0",
		"glob": "^8.0.3",
		"mocha": "^10.1.0",
		"typescript": "^4.9.3",
		"@vscode/test-electron": "^2.2.0"
	}
}
